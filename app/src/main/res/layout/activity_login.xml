<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginActivity"
    android:background="@drawable/login"
    >

    <TextView
        android:id="@+id/Userr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="7dp"
        android:text="Password"
        android:textColor="#101B4B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextLoginPassword"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextLoginUsername"
        tools:ignore="DuplicateIds" />

    <EditText
        android:id="@+id/editTextLoginPassword"
        android:layout_width="310dp"
        android:layout_height="45dp"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="400dp"
        android:background="@drawable/ashrectangle"
        android:backgroundTint="#519A8E8E"
        android:drawableEnd="@drawable/pwd"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/open_sans"
        android:hint="enter your Password"
        android:importantForAutofill="no"
        android:inputType="textPassword"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:password="true"
        android:textAlignment="viewStart"
        android:textColor="#96000000"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextLoginUsername"
        tools:ignore="Deprecated,DuplicateIds,HardcodedText,NotSibling,TextFields,VisualLintOverlap" />

    <EditText
        android:id="@+id/editTextLoginUsername"
        android:layout_width="310dp"
        android:layout_height="45dp"
        android:autofillHints=""
        android:background="@drawable/ashrectangle"
        android:backgroundTint="#519A8E8E"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/open_sans"
        android:hint="enter name"
        android:inputType="text"
        android:paddingStart="20dp"
        android:textAlignment="viewStart"
        android:textColor="#96000000"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.365"
        tools:ignore="DuplicateIds,HardcodedText,RtlSymmetry" />

    <TextView
        android:id="@+id/textViewForgetPwd"
        android:layout_width="243dp"
        android:layout_height="28dp"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/open_sans_semibold"
        android:text="  Forget your password ?"
        android:textAlignment="center"
        android:textColor="#101B4B"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLogin"
        app:layout_constraintVertical_bias="0.106"
        tools:ignore="HardcodedText,TextSizeCheck" />

    <TextView
        android:id="@+id/textViewNewUser"
        android:layout_width="150dp"
        android:layout_height="27dp"
        android:fontFamily="@font/open_sans_semibold"
        android:text="  New User ? "
        android:textAlignment="center"
        android:textColor="#101B4B"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewForgetPwd"
        app:layout_constraintVertical_bias="0.109"
        tools:ignore="HardcodedText,MissingConstraints,NotSibling,TextSizeCheck" />

    <Button
        android:id="@+id/buttonLogin"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginBottom="220dp"
        android:backgroundTint="#101B4B"
        android:fontFamily="@font/open_sans_semibold"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        android:text="Login"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextLoginPassword"
        app:layout_constraintVertical_bias="0.25"
        tools:ignore="HardcodedText,TextSizeCheck" />

    <TextView
        android:id="@+id/textViewSignUp"
        android:layout_width="90dp"
        android:layout_height="27dp"
        android:fontFamily="@font/alike"
        android:text="Sign up"
        android:textColor="#101B4B"
        android:textColorHighlight="#101B4B"
        android:textSize="22sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.738"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewForgetPwd"
        app:layout_constraintVertical_bias="0.109"
        tools:ignore="HardcodedText,MissingConstraints,NotSibling,TextSizeCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072"
        tools:ignore="ContentDescription,HardcodedText,ImageContrastCheck" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ContentDescription,HardcodedText,ImageContrastCheck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="107dp"
        android:layout_height="47dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="230dp"
        android:layout_marginBottom="42dp"
        android:text="LOGIN"
        android:textColor="#101B4B"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextLoginUsername"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/User"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="27dp"
        android:layout_marginBottom="4dp"
        android:text="User name"
        android:textColor="#101B4B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextLoginUsername"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>